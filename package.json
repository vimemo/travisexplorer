{
  "name": "travisexplorer",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@vimemo:vimemo/travisexplorer",
  "author": "",
  "license": "MIT",
  "scripts": {
    "test": "jest --coverage --projects jest-*.config.js",
    "test-coverage": "jest --coverage",
    "watch:test": "jest --watch",
    "build": "babel ./lib -d ./dist",
    "start": "node ./dist",
    "dev": "babel-node ./lib",
    "ci": "jest --coverage --projects jest-*.config.js && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.4.3",
    "babel-preset-env": "^1.6.1",
    "coveralls": "^3.0.0",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-jest": "^21.15.0",
    "eslint-plugin-prettier": "^2.6.0",
    "jest": "^22.4.3",
    "jest-runner-eslint": "^0.4.0",
    "jest-runner-prettier": "^0.2.2",
    "prettier": "^1.12.1"
  },
  "dependencies": {
    "curl-request": "^1.1.1",
    "express": "^4.16.3",
    "knex": "^0.14.6",
    "nodemon": "^1.17.3",
    "pg": "^7.4.1",
    "pg-format": "^1.0.4",
    "pouchdb": "^6.4.3",
    "ramda": "^0.25.0"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "<rootDir>/tests/**/*.js"
    ],
    "collectCoverageFrom": [
      "lib/**/*.{js}",
      "!lib/index.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "globalSetup": "<rootDir>/setup.js"
  }
}
