{
  "name": "travisexplorer",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@vimemo:vimemo/travisexplorer",
  "author": "",
  "license": "MIT",
  "scripts": {
    "test": "jest --coverage",
    "watch:test": "jest --watch",
    "build": "babel ./lib -d ./dist",
    "start": "node ./dist",
    "dev": "babel-node ./lib"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.4.3",
    "babel-preset-env": "^1.6.1",
    "coveralls": "^3.0.0",
    "jest": "^22.4.3"
  },
  "dependencies": {
    "curl-request": "^1.1.1",
    "express": "^4.16.3",
    "nodemon": "^1.17.3",
    "pouchdb": "^6.4.3",
    "ramda": "^0.25.0"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "lib/**/*.{js}",
      "!lib/index.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  }
}
